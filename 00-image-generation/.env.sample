# Microsoft Foundry Image Generation Configuration
# Copy this file to .env and fill in your values

# Your Foundry endpoint for image generation
FOUNDRY_IMAGE_ENDPOINT=https://your-resource.openai.azure.com/openai/v1/images/generations

# Your Foundry API key
FOUNDRY_API_KEY=your_api_key_here

# The deployment name for gpt-image-1.5 model
IMAGE_MODEL_DEPLOYMENT=gpt-image-1.5
